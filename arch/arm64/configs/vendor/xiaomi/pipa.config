# Device Specific Board Configuration
CONFIG_BOARD_PIPA=y

# Touchscreen
CONFIG_TOUCHSCREEN_NT36532_HOSTDL_SPI=y
# CONFIG_TOUCHSCREEN_NVT_DEBUG_FS is not set
CONFIG_TOUCHSCREEN_NEW_PEN_CONNECT_STRATEGY=y
CONFIG_TOUCHSCREEN_SUPPORT_NEW_GAME_MODE=y

# Enable PM8008 Regulator
CONFIG_REGULATOR_PM8008=y

# Enable MI US Proximity sensor
# CONFIG_US_PROXIMITY is not set

# Fingerprint
# CONFIG_INPUT_FINGERPRINT is not set

# Power supply drivers
# CONFIG_QPNP_FG_GEN4 is not set
CONFIG_QPNP_FG_GEN4_PIPA=y
# CONFIG_QPNP_SMB5 is not set
CONFIG_QPNP_SMB5_PIPA=y
CONFIG_BQ2597X_CHARGE_PUMP_PIPA=y
CONFIG_DUAL_BQ2597X=y
CONFIG_BATT_VERIFY_BY_DS28E16_PIPA=y
CONFIG_HL6111R=y
CONFIG_ONEWIRE_GPIO_PIPA=y
CONFIG_FUDA_1665=y
CONFIG_NO_PS_USB3=y

# Enable Dual KTZ8866 backlight driver
CONFIG_BACKLIGHT_KTZ8866=y

# Disable ESOC
# CONFIG_ESOC is not set

# Enable NANOSIC Keyboard
CONFIG_KEYBOARD_NANO_8030_V2=y

# Misc
CONFIG_MMHARDWARE_OTHER_DETECTION=y

# USB PD
# CONFIG_USB_PD_POLICY is not set
CONFIG_USB_PD_POLICY_DAGU=y

# Retry i2c operations on failure
CONFIG_I2C_RETRY=y
